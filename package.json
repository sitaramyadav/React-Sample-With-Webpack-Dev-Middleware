{
  "name": "rexpack",
  "version": "1.0.0",
  "description": "React Webpack dev middleware setup.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage",
    "build:dev": "rm -rf dist && webpack --mode development --config webpack.dev.config.js",
    "build:prod": "rm -rf dist && webpack --mode production --mode production --config webpack.prod.config.js",
    "start": "node server.js"
  },
  "repository": "https://github.com/sitaramyadav/React-Sample-With-Webpack-Dev-Middleware",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@testing-library/react": "^11.0.4",
    "babel-jest": "^26.3.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "eslint-plugin-react": "^7.12.1",
    "express": "^4.17.3",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.24.3"
  }
}
